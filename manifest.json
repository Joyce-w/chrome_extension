{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_title": "Hello Extensions",
        "default_popup": "hello.html",
        "default_icon": "./icons/extension_icon.png"
    }
}